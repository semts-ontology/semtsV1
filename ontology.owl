        <rdfs:domain rdf:resource="https://w3id.org/semts/ontology#KnowledgeGenerationInput"/>
        <rdfs:range rdf:resource="https://w3id.org/semts/ontology#TimeSeriesSegment"/>
        <rdfs:comment xml:lang="en">Defines the relation from a Knowledge Generation Input to a Time Series Segment and thus to metadata which specifies the underlying data.</rdfs:comment>
        <rdfs:label xml:lang="en">Input Data Reference</rdfs:label>
    </owl:ObjectProperty>
    


