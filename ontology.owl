        <rdfs:label xml:lang="en">Has Value</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/semts/ontology#inputDataReference -->


    <owl:ObjectProperty rdf:about="https://w3id.org/semts/ontology#inputDataReference">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
