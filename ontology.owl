        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/semts/ontology#Knowledge"/>
        <rdfs:range rdf:resource="https://w3id.org/semts/ontology#KnowledgeGenerationEntity"/>
        <rdfs:comment xml:lang="en">Relates the Knowledge Generation Entity, which produced this Knowledge instance.</rdfs:comment>
        <rdfs:label xml:lang="en">Knowledge Generation Entity</rdfs:label>
    </owl:ObjectProperty>
    


