        <rdfs:domain rdf:resource="https://w3id.org/semts/ontology#KnowledgeGenerationInput"/>
        <rdfs:range rdf:resource="https://w3id.org/semts/ontology#Knowledge"/>
        <rdfs:comment xml:lang="en">Similar to the Input Data Reference, this relation defines all the knowledge associated with the input of a Knowledge Generation Entity.</rdfs:comment>
        <rdfs:label xml:lang="en">Input Knowledge</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/semts/ontology#isPartOf -->


    <owl:ObjectProperty rdf:about="https://w3id.org/semts/ontology#isPartOf">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/isPartOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/semts/ontology#TimeSeriesSegment"/>
        <rdfs:range rdf:resource="https://w3id.org/semts/ontology#TimeSeriesSegment"/>
        <rdfs:comment xml:lang="en">Defines a hierarchic relationship between time series segments. The intuition of this property is to differentiate between complete time series, in a sense that it includes the data from start to end and subsegments which are reduced in time or dimensionality and thus can be considered a slice of this original data.</rdfs:comment>
        <rdfs:label xml:lang="en">Is Part Of</rdfs:label>
    </owl:ObjectProperty>
    


