        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/semts/ontology#TimeSeriesSegment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
        <rdfs:comment xml:lang="en">Defines the index associated with a Time Series Segment. Inherits from the time:TemporalEntity.</rdfs:comment>
        <rdfs:label xml:lang="en">Segment Index</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/semts/ontology#segmentKnowledge -->


    <owl:ObjectProperty rdf:about="https://w3id.org/semts/ontology#segmentKnowledge">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/semts/ontology#TimeSeriesSegment"/>
        <rdfs:range rdf:resource="https://w3id.org/semts/ontology#Knowledge"/>
        <rdfs:comment xml:lang="en">Relates any Knowledge available for a particular Time Series Segment.</rdfs:comment>
        <rdfs:label xml:lang="en">Segment Knowledge</rdfs:label>
    </owl:ObjectProperty>
    


