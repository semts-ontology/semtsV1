        <rdfs:domain rdf:resource="https://w3id.org/semts/ontology#Knowledge"/>
        <rdfs:range rdf:resource="https://w3id.org/semts/ontology#KnowledgeConcept"/>
        <rdfs:comment xml:lang="en">Relates a concept for a Knowledge instance. Can be used to assign concrete instances from the SemTS taxonomy.</rdfs:comment>
        <rdfs:label xml:lang="en">Knowledge Concept</rdfs:label>
    </owl:ObjectProperty>
    


