        <rdfs:range rdf:resource="https://w3id.org/semts/ontology#Knowledge"/>
        <rdfs:comment xml:lang="en">Opposed to Input Knowledge, this property defines the knowledge included in the output of a Knowledge generation Entity.</rdfs:comment>
        <rdfs:label xml:lang="en">Output Knowledge</rdfs:label>
    </owl:ObjectProperty>
    


