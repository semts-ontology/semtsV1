        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/semts/ontology#KnowledgeQualityMeasure"/>
        <rdfs:range rdf:resource="https://w3id.org/semts/ontology#QualityMeasureConcept"/>
        <rdfs:comment xml:lang="en">Defines the concept of a particular Knowledge Quality Measure.</rdfs:comment>
        <rdfs:label xml:lang="en">Quality Measure Concept</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/semts/ontology#segmentDimension -->


