    <owl:DatatypeProperty rdf:about="https://w3id.org/semts/ontology#mathExpressionFormat">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/semts/ontology#MathExpression"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">This property can be used to define the format of a mathematical expression. One example could be &quot;tex&quot; to define mathematical expressions defined via Latex.</rdfs:comment>
        <rdfs:label xml:lang="en">Math Expression Format</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/semts/ontology#mathExpressionParser -->


