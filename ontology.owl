        <rdfs:range rdf:resource="https://w3id.org/semts/ontology#KnowledgeQualityMeasure"/>
        <rdfs:comment xml:lang="en">Allows to assign a quality measure to instantiated knowledge.</rdfs:comment>
        <rdfs:label xml:lang="en">Knowledge Quality Measure</rdfs:label>
    </owl:ObjectProperty>
    


