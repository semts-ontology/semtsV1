        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">In case a mathematical expression can be parsed for evaluation, this property allows to specify a suitable parser solution.</rdfs:comment>
        <rdfs:label xml:lang="en">Math Expression Parser</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/semts/ontology#title -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/semts/ontology#title">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/title"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/semts/ontology#DataDimension"/>
        <rdfs:domain rdf:resource="https://w3id.org/semts/ontology#Knowledge"/>
        <rdfs:domain rdf:resource="https://w3id.org/semts/ontology#KnowledgeGenerationEntity"/>
        <rdfs:domain rdf:resource="https://w3id.org/semts/ontology#KnowledgeGenerationIO"/>
        <rdfs:domain rdf:resource="https://w3id.org/semts/ontology#KnowledgeQualityMeasure"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Inherits from dc:title and can be used to optionally add a title to an entity of the domain.</rdfs:comment>
        <rdfs:label xml:lang="en">Title</rdfs:label>
